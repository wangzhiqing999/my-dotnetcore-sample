@namespace W4101_AntDesignApp.Pages

@page "/myautocomplete"


<h3>测试输入-AutoComplete</h3>


<h3>简单使用的例子</h3>
<p>
    AutoComplete 不是 Select.
    <br />
    Select 是只能选择，不能自己输入一个全新的选项。
    <br />
    AutoComplete 是，你可以输入一个全新的数值。
    也可以从 AutoComplete 列表中选择。
    还可以从列表中选择后，再做点修改。
</p>

<AutoComplete @bind-Value="@_autoCompleteValue"
              Options="@_autoCompleteOptions"
              Placeholder="input here" />

<br />

<span>输入的数值： @_autoCompleteValue</span>




@code {


    private string _autoCompleteValue;

    List<string> _autoCompleteOptions = new List<string>()
        {
            "Beijing","Shanghai","Guangzhou","Shenzhen","Chongqing","Wuhan"
        };



}
